{
  "main": "server/index.js",
  "scripts": {
    "start": "NODE_PATH=./server nodemon server",
    "prod": "NODE_PATH=./server NODE_ENV=prod forever --minUptime 1000 --spinSleepTime 1000 start server",
    "uat": "NODE_PATH=./server NODE_ENV=uat forever --minUptime 1000 --spinSleepTime 1000 start server",
    "list": "forever list",
    "stop": "forever stop 0",
    "logs": "forever logs 0",
    "demo": "NODE_PATH=./server node server/scripts/demo",
    "timeform": "NODE_PATH=./server node server/timeform",
    "timeform-test": "NODE_PATH=./server mocha -t 10000 ./server/timeform/test.js",
    "test-server": "NODE_PATH=./server NODE_ENV=test nodemon server",
    "api-test": "NODE_PATH=./server mocha --require ./server/test/start.js --recursive $(find server/test -name '*.js')",
    "e2e": "run-p test-server api-test",
    "flush": "NODE_PATH=./server node server/scripts/flush",
    "populate": "NODE_PATH=./server run-s flush timeform demo messages",
    "messages": "NODE_PATH=./server node server/scripts/messages",
    "setup-search-dev": "NODE_PATH=./server node server/scripts/setupSearch",
    "setup-search-uat": "NODE_PATH=./server NODE_ENV=uat node server/scripts/setupSearch",
    "setup-search-prod": "NODE_PATH=./server NODE_ENV=prod node server/scripts/setupSearch",
    "test": "NODE_PATH=./server mocha -w --opts mocha.opts --recursive $(find server -name '*.spec.js')"
  },
  "dependencies": {
    "algoliasearch": "^3.24.0",
    "bcrypt-as-promised": "^1.1.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chai": "^4.0.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "config": "^1.26.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "forever": "^0.15.3",
    "fs-extra": "^3.0.1",
    "jsonwebtoken": "^7.4.1",
    "mime": "^1.3.6",
    "mocha": "^3.4.2",
    "mongoose": "^4.10.4",
    "mongoose-algolia": "^1.7.0",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  }
}
